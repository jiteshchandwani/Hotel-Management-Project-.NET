          setInterval(function () { document.getElementById('poperrforgotpass').style.visibility = "hidden"; }, 3000);

         document.getElementById('popnoreserve').style.visibility = "@ViewBag.nul";

         setInterval(function () { document.getElementById('popnoreserve').style.visibility = "hidden"; }, 3000);
