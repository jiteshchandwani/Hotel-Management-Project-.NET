                        @Html.ValidationMessage("CustomerName", null, new { @class = "text-danger", @style = "color:#ffae42" })
                    </td>
                </tr>

                <tr style="padding: 5px">
                    <td style="padding: 5px"><label for="dob"><b>Date of Birth</b></label></td>
                    <td style="padding: 5px">
                        @Html.TextBox("Date_of_Birth ", null, new { @class = "form-control", @type = "date", @max = "2018-01-01", @required = "required", @title = "Please Enter the correct Date" })
