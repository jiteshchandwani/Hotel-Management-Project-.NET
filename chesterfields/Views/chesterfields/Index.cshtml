                </tr>

                <tr style="padding: 5px">
                    <td style="padding: 5px"><label for="dob"><b>Gender</b></label></td>
                    @* <td style="padding: 5px">
                            <label class="radio-inline">
                                <input type="radio" name="optradio">Male
                            </label>
                            <label class="radio-inline">
                                <input type="radio" name="optradio">Female
                            </label>
                        </td>*@
                    <td>
                        @Html.DropDownList("Gender", new List<SelectListItem>
                                {
                                new SelectListItem {Text = "Male"},
                                new SelectListItem {Text = "Female"},
                                }, "Select Gender", new { @class = "form-control", @required = "required" })
                        @Html.ValidationMessage("Gender", null, new { @class = "text-danger", @style = "color:#ffae42" })
                    </td>
                </tr>


                <tr style="padding: 5px">

                    <td style="padding: 5px"><label for="adrs"><b>Address</b></label></td>
                    <td style="padding: 5px">
                        @Html.TextBox("Customer_Address", null, new { @class = "form-control", @type = "name", @pattern = "[A-Za-z0-9'\\.\\-\\s\\,]", @required = "required", @title = "Fill Correct Address" })
                        @Html.ValidationMessage("Customer_Address", null, new { @class = "text-danger", @style = "color:#ffae42" })
                    </td>

                <tr style="padding: 5px">
                    <td style="padding: 5px"><label for="City"><b>City</b></label></td>
                    <td style="padding: 5px">
                        @Html.TextBox("City", null, new { @class = "form-control", @type = "name", @pattern = "[A-Za-z0-9'\\.\\-\\s\\,]", @required = "required", @title = "Fill Correct City" })
                        @Html.ValidationMessage("City", null, new { @class = "text-danger", @style = "color:#ffae42" })
                    </td>

                <tr style="padding: 5px">
                    <td style="padding: 5px"><label for="pincode"><b>Pin Code</b></label></td>
                    <td style="padding: 5px">
                        @Html.TextBox("Pin_Code", null, new { @class = "form-control", @pattern = "^[0-9]{6}$", @required = "required", @title = "Fill Correct Pin Code" })
                        @Html.ValidationMessage("Pin_Code", null, new { @class = "text-danger", @style = "color:#ffae42" })
                    </td>

                <tr style="padding: 5px">
                    <td style="padding: 5px"><label for="country"><b>Country</b></label></td>
                    <td style="padding: 5px">
                        @Html.TextBox("Country", null, new { @class = "form-control", @pattern = "[A-Za-z0-9'\\.\\-\\s\\,]", @required = "required", @title = "Fill Correct Country" })
                        @Html.ValidationMessage("Country", null, new { @class = "text-danger", @style = "color:#ffae42" })
                    </td>

                <tr style="padding: 5px">
                    <td style="padding: 5px"><label for="phoneno"><b>Contact Number</b></label></td>
                    <td style="padding: 5px">

                        @Html.TextBox("Phone_Number", null, new { @class = "form-control", @type = "name", @pattern = "^[0-9]{10}$", @required = "required", @title = "Fill Correct Contact Number" })
                        @Html.ValidationMessage("Phone_Number", null, new { @class = "text-danger", @style = "color:#ffae42" })

                    </td>

                <tr style="padding: 5px">
                    <td style="padding: 5px"><label for="email"><b>Email</b></label></td>
                    <td style="padding: 5px">

                        @Html.TextBox("Email", null, new { @class = "form-control", @type = "name", @pattern = "^([a-zA-Z0-9_\\-\\.]+)@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.)|(([a-zA-Z0-9\\-]+\\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\\]?)$", @required = "required", @title = "Fill Correct Email" })
                        @Html.ValidationMessage("Email", null, new { @class = "text-danger", @style = "color:#ffae42" })
                    </td>

                <tr style="padding: 5px">
                    <td style="padding: 5px"><label for="username"><b>User Name</b></label></td>
                    <td style="padding: 5px">
                        @Html.TextBox("UserName", null, new { @class = "form-control", @type = "name", @pattern = "^[a-zA-Z]+[a-zA-Z0-9]*$", @required = "required", @title = "Username must start with letters and can contain letters and numbers only.", @autocomplete = "off" })
                        @Html.ValidationMessage("UserName", null, new { @class = "text-danger", @style = "color:#ffae42" })
                    </td>

                <tr style="padding: 5px">
                    <td style="padding: 5px"><label for="psw"><b>Password</b></label></td>
                    <td style="padding: 5px">
                        @Html.Password("Passwordd", null, new { @class = "form-control", @id = "txtNewPassword", @type = "password", @pattern = "^(?=.*[A-Z].*[A-Z])(?=.*[!@#$&*])(?=.*[0-9].*[0-9])(?=.*[a-z].*[a-z].*[a-z]).{8}$", @required = "required", @title = "Password must be 8 characters in length and must contain 2 uppercase letters, 3 lowercase letters, 2 numbers and 1 special character." })
                        @Html.ValidationMessage("Passwordd", null, new { @class = "text-danger", @style = "color:#ffae42" })

                    </td>

                <tr style="padding: 5px">
                    <td style="padding: 5px"><label for="psw-repeat"><b>Confirm Password</b></label></td>
                    <td style="padding: 5px">

                        @Html.Password("ConfirmPasswordd", null, new { @class = "form-control", @id = "txtConfirmPassword", @onChange = "checkPasswordMatch();", @type = "password", @pattern = "^(?=.*[A-Z].*[A-Z])(?=.*[!@#$&*])(?=.*[0-9].*[0-9])(?=.*[a-z].*[a-z].*[a-z]).{8}$", @required = "required", @title = "Password must be 8 characters in length and must contain 2 uppercase letters, 3 lowercase letters, 2 numbers and 1 special character." })
                        @Html.ValidationMessage("ConfirmPasswordd", null, new { @class = "text-danger", @style = "color:#ffae42" })
                    </td>
                </tr>
                <tr><td><div id="divCheckPasswordMatch"> </div></td> </tr>
            </TABLE>
            <script>

                function checkPasswordMatch() {
                    var password = $("#txtNewPassword").val();
                    var confirmPassword = $("#txtConfirmPassword").val();

                    if (password != confirmPassword)
                        $("#divCheckPasswordMatch").html("Passwords do not match!");
                    else
                        $("#divCheckPasswordMatch").html("Passwords match.");
                }

                $(document).ready(function () {
                    $("#txtConfirmPassword").keyup(checkPasswordMatch);
                });


            </script>

            <hr>
            <p>By creating an account you agree to our Terms & Privacy.</p>

            <center> <button type="submit" class="btn form-control" style="background-color:#D2AC67">Register</button></center>
        }
        <br />
        <br />
        <p>Already have an account? <a href="javascript:login('hideregandlog');">Sign in</a>.</p>
    </div>



    </form>
    <center><a href="javascript:login('hidereg');"><i class="fa fa-remove"></i></a></center>
</div>
